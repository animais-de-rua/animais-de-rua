{
    "private": true,
    "main": "index.html",
    "scripts": {
      "start": "npm run clear-cache && parcel parcel/index.html --open",
      "build": "npm run clear-cache && parcel build parcel/index.html --no-source-maps && npm run copy-public",
      "clear-cache": "del-cli --force ./.cache && del-cli --force ./dist",
      "copy-public": "cp -r ./parcel/public/* ./dist"
    },
    "devDependencies": {
      "cantil": "^1.0.1",
      "cross-env": "^7.0.3",
      "easing-utils": "0.0.7",
      "eslint": "^7.18.0",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-plugin-import": "^2.22.1",
      "laravel-mix": "^6.0.10",
      "milligram": "^1.4.1",
      "postcss": "^8.2.4",
      "resolve-url-loader": "^4.0.0",
      "sass": "^1.32.5",
      "sass-loader": "^12.1.0",
      "sass-resources-loader": "^2.1.1",
      "@babel/core": "^7.12.3",
      "del-cli": "^4.0.0",
      "normalize.css": "8.0.1",
      "parcel-bundler": "^1.12.4",
      "posthtml": "^0.16.4",
      "posthtml-expressions": "^1.5.0",
      "posthtml-include": "^1.5.0"
    },
    "keywords": []
  }
  